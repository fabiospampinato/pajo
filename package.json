{
  "name": "paketo",
  "repository": "github:fabiospampinato/paketo",
  "description": "A tiny library for importing your package.json, with proper types!",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "tsex clean",
    "compile": "tsex compile",
    "compile:watch": "tsex compile --watch",
    "test": "tsex test",
    "test:watch": "tsex test --watch",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "keywords": [
    "package.json"
  ],
  "dependencies": {
    "find-up-json": "^2.0.1",
    "tiny-dirname": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "fava": "^0.0.7",
    "tsex": "^1.1.4",
    "typescript": "^4.9.4"
  }
}
